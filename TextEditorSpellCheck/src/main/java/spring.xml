<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"> 
   
    <!-- create object of TextEditor and SpellCheck -->
    
      
    <bean id="Spellcheckobj" class="com.edu.SpellCheck"></bean>
    <bean id="TextEditorobj" class="com.edu.TextEditor">
     <property name="Spellcheck" ref="Spellcheckobj"></property>
     
     
    <!-- Using byName
    <bean id="TextEditorobj" class="com.edu.TextEditor" autowire="byName">
