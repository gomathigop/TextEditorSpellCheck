    <!-- using byType
    <bean id="TextEditorobj" class="com.edu.TextEditor" autowire="byType">
     -->
    <!--  
    <bean id="Spellcheckobj" class="com.edu.SpellCheck"></bean>
    <bean id="TextEditorobj" class="com.edu.TextEditor" autowire="constructor">
    -->
    </bean>
    </beans>


